{
  "name": "MasterNG",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Angular4.x",
  "scripts": {
    "clean": "rimraf dist",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:app",
    "build:app": "npm run clean && ng build --prod",
    "build:lib": "npm run clean && ngc && copyfiles package.json README.md LICENSE dist/",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage",
    "test-ci": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub": "npm run pub:lib && npm run pub:demo",
    "pub:demo": "npm run build:demo -- --base-href /ngx-treeview/ && gh-pages -d dist/demo",
    "pub:lib": "npm run build:lib && npm publish dist"
  },
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "MasterNG",
    "i18n"
  ],
  "author": "Mizi Lin<amily4555@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amily4555/MasterNG.git"
  },
  "bugs": {
    "url": "https://github.com/amily4555/MasterNG/issues"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "peerDependencies": {
    "@angular/common": "4.3.0",
    "@angular/core": "4.3.0",
    "@angular/forms": "4.3.0",
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/common": "4.3.0",
    "@angular/compiler": "4.3.0",
    "@angular/compiler-cli": "4.3.0",
    "@angular/core": "4.3.0",
    "@angular/forms": "4.3.0",
    "@angular/http": "4.3.0",
    "@angular/platform-browser": "4.3.0",
    "@angular/platform-browser-dynamic": "4.3.0",
    "@angular/router": "4.3.0",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.62",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "gh-pages": "^0.12.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.4",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "zone.js": "^0.8.5"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "mzmu": "^1.8.4",
    "normalize.css": "^7.0.0"
  }
}
